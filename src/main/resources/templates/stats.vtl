      #end
    </table>
  </div>
</div>
<script type="text/javascript">

  $('.table').DataTable();
  $('#qb-table-box').show();

  $('#qb').click(function() {
    $('.table-box').hide();
    $('#qb-table-box').show();
  });
  $('#rb').click(function() {
    $('.table-box').hide();
    $('#rb-table-box').show();
  });
  $('#wr').click(function() {
    $('.table-box').hide();
    $('#wr-table-box').show();
  });
  $('#te').click(function() {
    $('.table-box').hide();
    $('#te-table-box').show();
  });

</script>
