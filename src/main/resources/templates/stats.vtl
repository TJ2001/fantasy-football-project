<div class="container">
  <div class="form-group">
      <label class="radio-inline">
        <input type="radio" name="position" id="qb" checked>
        Quarterback
      </label>
      <label class="radio-inline">
        <input type="radio" name="position" id="rb">
        Running Back
      </label>
      <label class="radio-inline">
        <input type="radio" name="position" id="wr">
        Wide Receiver
      </label>
      <label class="radio-inline">
        <input type="radio" name="position" id="te">
        Tight End
      </label>
  </div>
  <div class="table-box" id="qb-table-box">
    <h2>Quarterbacks</h2>
    <table class="table table-striped table-bordered" cellspacing="0" width="100%">
      <thead>
        <tr>
          <th>Rank</th>
          <th>Name</th>
          <th>Team</th>
          <th>Comp</th>
          <th>Att</th>
          <th>Comp%</th>
          <th>Yards</th>
          <th>Yards/gm</th>
          <th>PassTD</th>
          <th>Int</th>
          <th>Sacks</th>
          <th>SackYds</th>
          <th>QBR</th>
          <th>Rush</th>
          <th>Yards</th>
          <th>Yards/gm</th>
          <th>RushTD</th>
          <th>Fumbles</th>
          <th>FumLost</th>
          <th>Games</th>
        </tr>
      </thead>
      #foreach($player in $players)
        <tr>
          <td>Rank</td>
          <td>$player.getFirstName() $player.getLastName()</td>
          <td>$player.getTeam()</td>
          <td>$player.getPasscompletions()</td>
          <td>$player.getPassattempts()</td>
          <td>$player.getPasspct()</td>
          <td>$player.getPassyards()</td>
          <td>$player.getPassyardspergame()</td>
          <td>$player.getPasstd()</td>
          <td>$player.getPassint()</td>
          <td>$player.getSacks()</td>
          <td>$player.getSackYds()</td>
          <td>$player.getQbrating()</td>
          <td>$player.getRushattempts()</td>
          <td>$player.getRushyards()</td>
          <td>$player.getRushyardspergame()</td>
          <td>$player.getRushtd()</td>
          <td>$player.getFumbles()</td>
          <td>$player.getFumlost()</td>
          <td>$player.getGamesplayed()</td>
      #end
    </table>
  </div>
  <div class="table-box" id="rb-table-box">
    <h2>Running Backs</h2>
    <table class="table table-striped table-bordered" cellspacing="0" width="100%">
      <thead>
        <tr>
          <th>Rank</th>
          <th>Name</th>
          <th>Team</th>
          <th>Att</th>
          <th>Att/G</th>
          <th>Yds</th>
          <th>Avg</th>
          <th>Yds/G</th>
          <th>TD</th>
          <th>1st</th>
          <th>1st%</th>
          <th>Lng</th>
          <th>20+</th>
          <th>40+</th>
          <th>FUM</th>
        </tr>
      </thead>
      #foreach($player in $players)
        <tr>
          <td>Rank</td>
          <td>Name</td>
          <td>Team</td>
          <td>Att</td>
          <td>Att/G</td>
          <td>Yds</td>
          <td>Avg</td>
          <td>Yds/G</td>
          <td>TD</td>
          <td>1st</td>
          <td>1st%</td>
          <td>Lng</td>
          <td>20+</td>
          <td>40+</td>
          <td>FUM</td>
        </tr>
      #end
    </table>
  </div>
  <div class="table-box" id="wr-table-box">
    <h2>Wide Receivers</h2>
    <table class="table table-striped table-bordered" cellspacing="0" width="100%">
      <thead>
