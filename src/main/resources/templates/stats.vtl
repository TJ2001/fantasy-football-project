          <th>Lng</th>
          <th>20+</th>
          <th>40+</th>
          <th>FUM</th>
        </tr>
      </thead>
      #foreach($player in $players)
        <tr>
          <td>Rank</td>
          <td>Name</td>
          <td>Team</td>
          <td>Rec</td>
          <td>Yds</td>
          <td>Avg</td>
          <td>Yds/G</td>
