<div class="stats-background">
  <div class="container stats">
    <div class="form-group">
      <label class="radio-inline">
        <input type="radio" name="position" id="qb" checked>
        Quarterback
      </label>
      <label class="radio-inline">
        <input type="radio" name="position" id="rb">
        Running Back
      </label>
      <label class="radio-inline">
        <input type="radio" name="position" id="wr">
        Wide Receiver
      </label>
      <label class="radio-inline">
        <input type="radio" name="position" id="te">
        Tight End
      </label>
      <label class="radio-inline">
        <input type="radio" name="position" id="k">
        Kicker
      </label>
      <label class="radio-inline">
        <input type="radio" name="position" id="de">
        Defense
      </label>
    </div>
    <div class="table-box" id="qb-table-box">
      <h2>Quarterbacks</h2>
      <table id="qb-table" class="table table-striped table-bordered" cellspacing="0" width="100%">
        <thead>
          <tr>
            <th>Rank</th>
            <th>Name</th>
            <th>Team</th>
            <th>Comp</th>
            <th>Att</th>
            <th>Yds</th>
            <th>PassTD</th>
            <th>Int</th>
            <th>Sacks</th>
            <th>QBR</th>
            <th>Rush</th>
            <th>Yds</th>
            <th>RushTD</th>
          </tr>
        </thead>
        #foreach($qb in $qbs)
        <tr>
          <td>Rank</td>
          <td><a href="/players/$qb.getPlayerId()">$qb.getFirstName() $qb.getLastName()</a></td>
          <td><img class="teamlogo" src="$qb.getTeamImg()"> $qb.getTeam()</td>
          <td>$qb.getPassCompletions()</td>
          <td>$qb.getPassAttempts()</td>
          <td>$qb.getPassYards()</td>
          <td>$qb.getPassTd()</td>
          <td>$qb.getPassInt()</td>
          <td>$qb.getSacks()</td>
          <td>$qb.getQbrating()</td>
          <td>$qb.getRushAttempts()</td>
          <td>$qb.getRushYards()</td>
          <td>$qb.getRushTd()</td>
        </tr>
        #end
      </table>
    </div>
    <div class="table-box" id="rb-table-box">
      <h2>Running Backs</h2>
      <table id="rb-table" class="table table-striped table-bordered" cellspacing="0" width="100%">
        <thead>
          <tr>
            <th>Rank</th>
            <th>Name</th>
            <th>Team</th>
            <th>Att</th>
            <th>Yds</th>
            <th>Yds/G</th>
            <th>Avg</th>
            <th>RushTD</th>
            <th>RecTD</th>
            <th>AvgRecYds</th>
            <th>Rec</th>
            <th>Fum</th>
          </tr>
        </thead>
        #foreach($rb in $rbs)
        <tr>
          <td>Rank</td>
          <td><a href="/players/$rb.getPlayerId()">$rb.getFirstName() $rb.getLastName()</a></td>
          <td><img class="teamlogo" src="$rb.getTeamImg()"> $rb.getTeam()</td>
          <td>$rb.getRushAttempts()</td>
          <td>$rb.getRushYards()</td>
          <td>$rb.getRushYardsPerGame()</td>
          <td>$rb.getRushAverage()</td>
          <td>$rb.getRushTd()</td>
          <td>$rb.getReceptionTd()</td>
          <td>$rb.getAverageReceptionYardage()</td>
          <td>$rb.getReceptions()</td>
          <td>$rb.getRushFumbles()</td>
        </tr>
        #end
      </table>
    </div>
    <div class="table-box" id="wr-table-box">
      <h2>Wide Receivers</h2>
      <table id="wr-table" class="table table-striped table-bordered" cellspacing="0" width="100%">
        <thead>
          <tr>
            <th>Rank</th>
            <th>Name</th>
            <th>Team</th>
            <th>Rec</th>
            <th>Yds</th>
            <th>Avg</th>
            <th>Targ</th>
            <th>TD</th>
          </tr>
        </thead>
        #foreach($wr in $wrs)
        <tr>
          <td>Rank</td>
          <td><a href="/players/$wr.getPlayerId()">$wr.getFirstName() $wr.getLastName()</a></td>
          <td><img class="teamlogo" src="$wr.getTeamImg()"> $wr.getTeam()</td>
          <td>$wr.getReceptions()</td>
          <td>$wr.getRecyards()</td>
          <td>$wr.getRecyardspergame()</td>
          <td>$wr.getTargets()</td>
          <td>$wr.getRectd()</td>
        </tr>
        #end
      </table>
    </div>
    <div  class="table-box" id="te-table-box">
      <h2>Tight Ends</h2>
      <table id="te-table" class="table table-striped table-bordered" cellspacing="0" width="100%">
        <thead>
          <tr>
            <th>Rank</th>
            <th>Name</th>
            <th>Team</th>
            <th>Rec</th>
            <th>Yds</th>
            <th>Avg</th>
            <th>Targ</th>
            <th>TD</th>
          </tr>
        </thead>
        #foreach($te in $tes)
        <tr>
          <td>Rank</td>
          <td><a href="/players/$te.getPlayerId()">$te.getFirstName() $te.getLastName()</a></td>
          <td><img class="teamlogo" src="$te.getTeamImg()"> $te.getTeam()</td>
          <td>$te.getReceptions()</td>
          <td>$te.getRecyards()</td>
          <td>$te.getRecyardspergame()</td>
          <td>$te.getTargets()</td>
          <td>$te.getRectd()</td>
        </tr>
        #end
      </table>
    </div>
    <div  class="table-box" id="k-table-box">
      <h2>Kickers</h2>
      <table id="k-table" class="table table-striped table-bordered" cellspacing="0" width="100%">
        <thead>
          <tr>
            <th>Rank</th>
            <th>Name</th>
            <th>Team</th>
            <th>XP</th>
            <th>XP%</th>
            <th>FG</th>
            <th>FG%</th>
            <th>FG40-49%</th>
            <th>FGLng</th>
            <th>FGBlk</th>
          </tr>
        </thead>
        #foreach($k in $ks)
        <tr>
          <td>Rank</td>
          <td><a href="/players/$k.getPlayerId()">$k.getFirstName() $k.getLastName()</a></td>
          <td><img class="teamlogo" src="$k.getTeamImg()"> $k.getTeam()</td>
          <td>$k.getXpmade()</td>
          <td>$k.getXppct()</td>
          <td>$k.getFgmade()</td>
          <td>$k.getFgpct()</td>
          <td>$k.getFg40_49pct()</td>
          <td>$k.getFglng()</td>
          <td>$k.getFgblk()</td>
        </tr>
        #end
      </table>
    </div>
    <div  class="table-box" id="de-table-box">
      <h2>Defense</h2>
      <table id="de-table" class="table table-striped table-bordered" cellspacing="0" width="100%">
        <thead>
          <tr>
            <th>Rank</th>
            <th>Team Name</th>
            <th>Tackles</th>
            <th>Sacks</th>
            <th>PontDef</th>
            <th>Stuffs</th>
            <th>Int</th>
            <th>ForFum</th>
            <th>TDFromFum</th>
            <th>PtsAG</th>
          </tr>
        </thead>
        #foreach($de in $des)
        <tr>
          <td>Rank</td>
          <td><img class="teamlogo" src="$de.getTeamImg()"> $de.getTeamName()</td>
          <td>$de.getTotalTackles()</td>
          <td>$de.getSacks()</td>
          <td>$de.getPd()</td>
          <td>$de.getStuffs()</td>
          <td>$de.getInterceptions()</td>
          <td>$de.getForcedFumble()</td>
          <td>$de.getTdFromFumble()</td>
          <td>$de.getPointsAgainst()</td>
        </tr>
        #end
      </table>
    </div>
  </div>
  <script src="/datatables.js"></script>
</div>
